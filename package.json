{
  "name": "ddd-base",
  "version": "1.0.0",
  "description": "Bassic DDD Express",
  "keywords": [
    "express",
    "lambda",
    "grpc",
    "DDD",
    "Boilerplate"
  ],
  "author": "Gustavo S. Rodrigues <gustavo.beavis@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/gustavobeavis/DDDBase",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "start": "./src/index.js",
    "start:grpc": "cross-env NODE_ENV=local babel-node ./src/grpc.js",
    "start:grpc:builded": "cross-env NODE_ENV=local  node ./dist/grpc.js",
    "start:grpc:debug": "cross-env NODE_ENV=local babel-node ./src/grpc.js --debug-port 5858",
    "prestart:grpc:dev": "./node_modules/.bin/pm2 delete ecosystemGrpc.json; ",
    "start:grpc:dev": "pm2 start --no-daemon -s ecosystemGrpc.json",
    "stop:grpc:dev": "./node_modules/.bin/pm2 delete ecosystemGrpc.json",
    "start:http": "cross-env NODE_ENV=local babel-node ./src/index.js",
    "start:http:builded": "cross-env NODE_ENV=local  node ./dist/index.js",
    "start:http:debug": "cross-env NODE_ENV=local babel-node ./src/index.js --debug-port 5858",
    "prestart:http:dev": "./node_modules/.bin/pm2 delete ecosystemHttp.json; ",
    "start:http:dev": "pm2 start --no-daemon -s ecosystemHttp.json",
    "stop:http:dev": "./node_modules/.bin/pm2 delete ecosystemHttp.json",
    "prestart:serverless": "npm run build:serverless",
    "start:serverless": "sam local start-api",
    "prestart:serverless:debug": "npm run build:serverless",
    "start:serverless:debug": "sam local start-api --debug-port 5858",
    "kill:dev": "./node_modules/.bin/pm2 kill",
    "db:migrate": "cross-env NODE_ENV=local ./node_modules/.bin/sequelize db:migrate",
    "db:migrate:destroy": "cross-env NODE_ENV=local ./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seeder": "cross-env NODE_ENV=local ./node_modules/.bin/sequelize db:seed:all",
    "db:seeder:destroy": "cross-env NODE_ENV=local ./node_modules/.bin/sequelize db:seed:undo:all",
    "prebuild": "rm -rf ./dist",
    "build": "./node_modules/.bin/babel ./src --out-dir dist/",
    "prebuild:serverless": "rm -rf ./dist",
    "build:serverless": "./node_modules/.bin/babel ./src --out-dir dist/",
    "postbuild:serverless": "cp -R ./node_modules/ ./dist/node_modules/",
    "pretest": "rm -f ./database/*.test.*; cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:migrate; cross-env NODE_ENV=test node_modules/.bin/sequelize db:seed:all",
    "test": "cross-env NODE_ENV=test mocha test --exit",
    "postest": "npm run clean:build",
    "cover": "cross-env NODE_ENV=test nyc mocha test --exit",
    "coverreport": "nyc report --reporter=lcov",
    "precoveralls": "mkdir coverage",
    "coveralls": "npm run cover -- --report lcovonly;npm run coverreport; cat ./coverage/lcov.info",
    "clean": "rm -f package-lock.json && rm -rf node_modules &&  rm -rf ./dist/ ",
    "clean:build": "rm -rf ./dist/",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix"
  },
  "nyc": {
    "exclude": [
      "test",
      "dist",
      "database",
      "log",
      "public",
      "docs",
      "./src/domain",
      "./src/config",
      "./src/http/middlewares/jwt.js"
    ]
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.0",
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "consign": "^0.1.6",
    "cors": "^2.8.4",
    "cpf": "^1.0.6",
    "express": "^4.16.3",
    "express-winston": "^4.0.0",
    "grpc": "^1.17.0",
    "helmet": "^3.12.0",
    "js-yaml": "^3.12.1",
    "moment": "^2.21.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.3.0",
    "sqlite3": "^4.0.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "babel-eslint": "10.0.2",
    "babel-plugin-istanbul": "5.1.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "mocha": "5.2.0",
    "nyc": "15.1.0",
    "pm2": "4.1.2",
    "sequelize-cli": "5.5.0",
    "supertest": "4.0.2"
  }
}
