{
	"name": "ddd-base",
	"version": "1.0.0",
	"description": "Bassic DDD Express",
	"keywords": [ "express", "lambda", "DDD", "Boilerplate" ],
	"author": "Gustavo S. Rodrigues <gustavo.beavis@gmail.com>",
	"license": "MIT",
	"repository": "https://github.com/gustavobeavis/DDDBase",
	"main": "./dist/index.js",
	"engines": {
		"node": ">=6.9.0"
	},
	"scripts": {
		"start": "./src/index.js",
		"start:server:local": "cross-env NODE_ENV=local babel-node ./src/index.js",
		"prestart:serverless": "npm run build:serverless",
		"start:serverless": "sam local start-api",
		"prestart:serverless:debug": "npm run build:serverless",
		"start:serverless:debug": "sam local start-api --debug-port 5858",
		"prestart:dev": "./node_modules/.bin/pm2 delete ecosystem.json; ",
		"start:dev": "pm2 start --no-daemon -s ecosystem.json",
		"start:dev:stop": "./node_modules/.bin/pm2 delete ecosystem.json",
		"start:dev:kill": "./node_modules/.bin/pm2 kill",
		"start:builded": "cross-env NODE_ENV=local  node ./dist/index.js",
		"db:migrate": "cross-env NODE_ENV=local ./node_modules/.bin/sequelize db:migrate",
		"db:migrate:destroy": "cross-env NODE_ENV=local ./node_modules/.bin/sequelize db:migrate:undo:all",
		"db:seeder": "cross-env NODE_ENV=local ./node_modules/.bin/sequelize db:seed:all",
		"db:seeder:destroy": "cross-env NODE_ENV=local ./node_modules/.bin/sequelize db:seed:undo:all",
		"prebuild": "rm -rf ./dist",
		"build": "./node_modules/.bin/babel ./src --out-dir dist/",
		"prebuild:serverless": "rm -rf ./dist",
		"build:serverless": "./node_modules/.bin/babel ./src --out-dir dist/",
		"postbuild:serverless": "cp -R ./node_modules/ ./dist/node_modules/",
		"debug": "babel-node ./src/index.js --nolazy --inspect=0.0.0.0:9229",
		"pretest": "rm -f ./database/*.test.*; cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:migrate; cross-env NODE_ENV=test node_modules/.bin/sequelize db:seed:all",
		"test": "cross-env NODE_ENV=test mocha test --exit",
		"postest": "npm run clean:build",
		"cover": "cross-env NODE_ENV=test nyc mocha test --exit",
		"coverreport": "nyc report --reporter=lcov",
		"precoveralls": "mkdir coverage",
		"coveralls": "npm run cover -- --report lcovonly;npm run coverreport; cat ./coverage/lcov.info | coveralls",
		"clean": "rm -f package-lock.json && rm -rf node_modules &&  rm -rf ./dits/ ",
		"clean:build": "rm -rf ./dist/",
		"lint": "eslint src/ test/",
		"lint:fix": "eslint src/ test/ --fix"
	},
	"nyc": {
		"exclude": [
			"test",
			"dist",
			"database",
			"log",
			"public",
			"docs",
			"./src/domain",
			"./src/config",
			"./src/http/middlewares/jwt.js"
		]
	},
	"dependencies": {
		"aws-serverless-express": "^3.3.5",
		"babel-preset-env": "^1.6.1",
		"babel-preset-stage-3": "^6.24.1",
		"bcrypt": "^3.0.0",
		"body-parser": "^1.18.2",
		"compression": "^1.7.3",
		"consign": "^0.1.6",
		"cors": "^2.8.4",
		"cpf": "^1.0.6",
		"crypto-js": "^3.1.9-1",
		"express": "^4.16.3",
		"express-winston": "^3.0.0",
		"helmet": "^3.12.0",
		"moment": "^2.21.0",
		"passport": "^0.4.0",
		"passport-hash": "^0.6.1",
		"passport-http": "^0.3.0",
		"passport-jwt": "^4.0.0",
		"pg": "^7.4.0",
		"pg-hstore": "^2.3.2",
		"sequelize": "^4.22.11",
		"sqlite3": "^4.0.2",
		"winston": "^3.0.0"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-core": "6.26.3",
		"babel-eslint": "10.0.1",
		"babel-plugin-istanbul": "5.1.0",
		"babel-register": "6.26.0",
		"coveralls": "3.0.2",
		"cross-env": "5.2.0",
		"eslint": "5.11.1",
		"eslint-config-standard": "12.0.0",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-node": "8.0.1",
		"eslint-plugin-promise": "4.0.1",
		"eslint-plugin-standard": "4.0.0",
		"istanbul": "0.4.5",
		"mocha": "5.2.0",
		"nyc": "13.0.1",
		"sequelize-cli": "5.4.0",
		"pm2": "3.2.2",
		"supertest": "3.3.0"
	}
}
